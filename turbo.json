{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "type-defs#typegen": {
      "outputs": ["dist/**"]
    },
    "type-defs#build": {
      "outputs": ["dist/**"],
      "dependsOn": ["typegen"]
    },
    "lint": {
      "dependsOn": ["^build"]
    },
    "start": {
      "dependsOn": ["build"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "deploy": {
      "dependsOn": ["^build"]
    }
  }
}
