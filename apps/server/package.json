{
  "name": "server",
  "version": "0.1.0-alpha",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf dist",
    "dev": "nest start --watch",
    "build": "nest build",
    "start": "nest start",
    "start:prod": "pnpm migrate:run && pnpm start",
    "clean": "rm -rf dist",
    "deploy": "pnpx now --prod",
    "typeorm": "ts-node -r tsconfig-paths/register --transpile-only ./node_modules/typeorm/cli.js -d src/database/data-source.ts",
    "typegen": "tsc -p tsconfig.typegen.json && rm -rf ../../packages/type-defs/src/dts && mv -f dts ../../packages/type-defs/src/dts",
    "migrate:generate": "func() { pnpm typeorm migration:generate src/database/migrations/\"$1\"; }; func",
    "migrate:run": "pnpm typeorm migration:run",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix"
  },
  "dependencies": {
    "type-defs": "workspace:*",
    "@nestjs/axios": "^1.0.1",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/terminus": "^9.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@ngneat/falso": "^6.4.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.15",
    "@types/lodash.merge": "^4.6.7",
    "@types/lodash.mergewith": "^4.6.7",
    "@types/lodash.omit": "^4.5.7",
    "@types/lodash.uniqby": "^4.7.7",
    "@types/node": "^18.11.18",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/validator": "^13.7.10",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.0",
    "cloudinary": "^1.33.0",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "helmet": "^6.0.1",
    "lodash.merge": "^4.6.2",
    "lodash.mergewith": "^4.6.2",
    "lodash.omit": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "stripe": "^11.6.0",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.11",
    "typeorm-naming-strategies": "^4.1.0",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "dts-gen": "^0.6.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.19.1",
    "prettier": "^2.8.2",
    "tsconfig": "workspace:*",
    "tsconfig-paths": "^4.1.2"
  },
  "engines": {
    "node": ">=16"
  }
}
